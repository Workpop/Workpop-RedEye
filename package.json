{
  "name": "@workpop/redeye",
  "version": "0.0.1",
  "description": "GraphQL Util Library ",
  "main": "dist/index.js",
  "scripts": {
    "runQuery": "babel-node src/index.js",
    "test": "mocha test/index.js",
    "lint": "eslint .",
    "fix": "eslint . --fix",
    "prepublish": "babel ./src --ignore test --out-dir ./dist"
  },
  "keywords": [
    "GraphQL"
  ],
  "devDependencies": {
    "@workpop/eslint-config-workpop": "0.0.5",
    "babel-cli": "^6.6.5",
    "babel-core": "6.3.21",
    "babel-eslint": "^6.0.0-beta.6",
    "babel-loader": "6.2.0",
    "babel-plugin-syntax-flow": "^6.8.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "eslint": "^3.4.0",
    "mocha": "^3.0.2",
    "sinon": "^1.17.5"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "@workpop/eslint-config-workpop"
    ],
    "rules": {
      "newline-per-chained-call": 0,
      "react/jsx-filename-extension": 0,
      "max-len": 0,
      "no-use-before-define": 0,
      "arrow-body-style": 0,
      "dot-notation": 0,
      "no-console": 0
    },
    "env": {
      "mocha": true
    }
  },
  "author": "Workpop",
  "license": "ISC",
  "dependencies": {
    "graphql-tag": "^0.1.13",
    "lodash": "^4.15.0",
    "request-promise": "^4.1.1"
  }
}
